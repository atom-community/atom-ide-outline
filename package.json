{
  "name": "atom-ide-outline",
  "main": "./dist/main",
  "version": "1.3.1",
  "description": "Outline UI compatible with IDE packages",
  "repository": "https://github.com/atom-ide-community/atom-ide-outline",
  "keywords": [
    "atom-package",
    "atom-ide",
    "atom-outline",
    "ide-outline",
    "outline"
  ],
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix --ext js,ts,coffee,json",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "babel": "npm run clean && shx cp -r src dist && env NODE_ENV=development env BABEL_ENV=development babel dist --out-dir dist",
    "dev": "npm run clean && env NODE_ENV=development env BABEL_ENV=production rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production env BABEL_ENV=production rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./spec/runner",
  "dependencies": {},
  "devDependencies": {
    "@types/atom": "latest",
    "@types/node": "^14.0.20",
    "typescript": "^3.9.6",
    "tslib": "^2.0.0",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.11",
    "atom-jasmine3-test-runner": "^4.4.8",
    "prettier": "^2.0.5",
    "eslint": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "latest",
    "eslint-plugin-coffee": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-only-warn": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "tslint": "latest",
    "tslint-config-prettier": "latest",
    "rollup": "latest",
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "@rollup/plugin-typescript": "latest",
    "rollup-plugin-coffee-script": "latest",
    "rollup-plugin-babel": "^4.4.0",
    "@rollup/plugin-json": "latest",
    "rollup-plugin-terser": "latest",
    "shx": "latest",
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-decorators": "latest",
    "@babel/plugin-proposal-do-expressions": "latest",
    "@babel/plugin-proposal-export-default-from": "latest",
    "@babel/plugin-proposal-export-namespace-from": "latest",
    "@babel/plugin-proposal-function-bind": "latest",
    "@babel/plugin-proposal-function-sent": "latest",
    "@babel/plugin-proposal-json-strings": "latest",
    "@babel/plugin-proposal-logical-assignment-operators": "latest",
    "@babel/plugin-proposal-nullish-coalescing-operator": "latest",
    "@babel/plugin-proposal-numeric-separator": "latest",
    "@babel/plugin-proposal-optional-chaining": "latest",
    "@babel/plugin-proposal-pipeline-operator": "latest",
    "@babel/plugin-proposal-throw-expressions": "latest",
    "@babel/plugin-syntax-dynamic-import": "latest",
    "@babel/plugin-syntax-import-meta": "latest",
    "@babel/plugin-transform-async-to-generator": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-flow": "latest",
    "@babel/preset-react": "latest",
    "npm-check-updates": "latest",
    "build-commit": "latest"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "outline-view": {
      "versions": {
        "0.1.0": "consumeOutlineProvider"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    }
  },
  "package-deps": {}
}
