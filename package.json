{
  "name": "atom-ide-outline",
  "main": "./dist/main",
  "version": "2.0.0",
  "description": "Outline UI compatible with IDE packages",
  "repository": "https://github.com/atom-ide-community/atom-ide-outline",
  "keywords": [
    "atom-package",
    "atom-ide",
    "atom-outline",
    "ide-outline",
    "outline"
  ],
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "test.format": "prettier . --check",
    "lint": "eslint . --fix",
    "test.lint": "eslint .",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "dev": "npm run clean && cross-env NODE_ENV=development rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./spec/runner",
  "dependencies": {
    "atom-ide-base": "^2.2.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/atom": "latest",
    "@types/node": "^14.14.31",
    "@types/lodash": "^4.14.168",
    "typescript": "^4.2.3",
    "tslib": "^2.1.0",
    "@types/jasmine": "^3.6.4",
    "atom-jasmine3-test-runner": "^5.1.9",
    "prettier": "^2.2.1",
    "eslint": "7.21.0",
    "eslint-config-atomic": "^1.12.2",
    "rollup": "^2.40.0",
    "rollup-plugin-atomic": "^2.1.0",
    "shx": "latest",
    "cross-env": "latest",
    "npm-check-updates": "latest",
    "build-commit": "latest"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "outline-view": {
      "versions": {
        "0.1.0": "consumeOutlineProvider"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    }
  },
  "package-deps": {}
}
