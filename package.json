{
  "name": "atom-ide-outline",
  "main": "./dist/main",
  "version": "1.12.0",
  "description": "Outline UI compatible with IDE packages",
  "repository": "https://github.com/atom-ide-community/atom-ide-outline",
  "keywords": [
    "atom-package",
    "atom-ide",
    "atom-outline",
    "ide-outline",
    "outline"
  ],
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "atom --test spec",
    "clean": "shx rm -rf dist",
    "babel": "npm run clean && shx cp -r src dist && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=false babel dist --out-dir dist",
    "dev": "npm run clean && cross-env NODE_ENV=development cross-env BABEL_KEEP_MODULES=true rollup -c -w",
    "build": "npm run clean && cross-env NODE_ENV=production cross-env BABEL_KEEP_MODULES=true rollup -c ",
    "build-commit": "build-commit -o dist",
    "bump": "ncu -u",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./spec/runner",
  "dependencies": {
    "atom-ide-base": "^1.8.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/atom": "latest",
    "@types/node": "^14.0.27",
    "@types/lodash": "^4.14.162",
    "typescript": "^3.9.7",
    "tslib": "^2.0.1",
    "@types/jasmine": "^3.5.12",
    "atom-jasmine3-test-runner": "^5.0.5",
    "prettier": "^2.0.5",
    "eslint": "7.6.0",
    "eslint-config-atomic": "^1.3.0",
    "rollup": "^2.23.1",
    "rollup-plugin-atomic": "^1.2.0",
    "shx": "latest",
    "cross-env": "latest",
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "babel-preset-atomic": "latest",
    "npm-check-updates": "latest",
    "build-commit": "latest"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "outline-view": {
      "versions": {
        "0.1.0": "consumeOutlineProvider"
      }
    },
    "busy-signal": {
      "versions": {
        "1.0.0": "consumeSignal"
      }
    }
  },
  "package-deps": {}
}
